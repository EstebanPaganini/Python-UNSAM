import csv
from pprint import pprint
def leer_arboles(nombre_archivo):
    with open(nombre_archivo, encoding='utf8') as f:
        arboleda = []
        cantidad = 0
        rows = csv.reader(f)
        titulos = next(rows)
        for e in rows:
            record = dict(zip(titulos, e))
            arboleda.append(record)
            cantidad +=1
    return arboleda

arboleda = leer_arboles('C:/ejercicios_python/data/arbolado.csv')

H=[(float(arbol['altura_tot']), float(arbol['diametro'])) for arbol in arboleda if arbol['nombre_com'] == 'Jacarand√°']
pprint(H)
